{
    "name": "@fincity/api-tester-webview",
    "version": "1.0.0",
    "description": "Webview for the api-tester-plugin",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "start": "parcel watch src/index.tsx --dist-dir ../api-tester/media"
    },
    "keywords": [
        "api-tester",
        "postman",
        "alternative"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ],
        "*.{html,css,less,ejs}": [
            "prettier --write",
            "git add"
        ]
    },
    "author": "Kiran Kumar Grandhi",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.12.0",
        "@babel/preset-typescript": "^7.18.6",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "eslint": "^8.21.0",
        "eslint-plugin-react": "^7.30.1",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "parcel": "^2.7.0",
        "prettier": "^2.7.1",
        "process": "^0.11.10"
    },
    "dependencies": {
        "@vscode/codicons": "^0.0.32",
        "@vscode/webview-ui-toolkit": "^1.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
